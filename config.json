{
  "memory": {
    "backend": "fs",
    "fs": {
      "root_dir": "syntheticMemory"
    },
    "vector": {
      "provider": "pinecone",
      "index": "jrdev-memory",
      "api_key": "env:PINECONE_API_KEY"
    }
  },
  "jira": {
    "base_url": "env:JIRA_BASE_URL",
    "token": "env:JIRA_TOKEN"
  },
  "pess": {
    "url": "env:PESS_URL",
    "api_key": "env:PESS_API_KEY"
  },
  "prompt_templates": {
    "repository_url": "https://github.com/ArnoldoM23/jr-dev-agent-prompt-templates",
    "auth_token": "env:GITHUB_TOKEN"
  },
  "prompt_builder": {
    "use_llm_assist": false,
    "llm_model": "gpt-4"
  }
}